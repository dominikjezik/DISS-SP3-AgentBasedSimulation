                                          RowHeight="24"
                                          BorderThickness="1" BorderBrush="Gray">
                    
                                    <DataGrid.Styles>
                                        <Style Selector="DataGridColumnHeader">
                                            <Setter Property="FontWeight" Value="Bold"/>
                                        </Style>
                                        <Style Selector="DataGridCell:current /template/ Grid#FocusVisual"> 
                                            <Setter Property="IsVisible" Value="false" /> 
                                        </Style> 
                                    </DataGrid.Styles>
                    
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="ID" Width="Auto" Binding="{Binding Id}" />
                                        <DataGridTextColumn Header="Utilization" Width="Auto" Binding="{Binding Utilization}" />
                                        <DataGridTextColumn Header="Utilization 95% CI" Width="*" Binding="{Binding State}" />
                                    </DataGrid.Columns>
                                </DataGrid>
                            </TabItem>
                        </TabControl>
                        
                    </Grid>
                </TabItem>
                <TabItem Header="Parameters" FontSize="16" FontWeight="Bold">
                    <Grid Margin="-4,10,0,0"
                        RowDefinitions="Auto,Auto,Auto,Auto,Auto,Auto">
                        <Label
                            Grid.Row="1" Grid.Column="0"
                            Margin="0,10,10,5"
                            FontSize="16"
                            FontWeight="Bold"
                            FontFamily="Source Code Pro"
                            Content="Assembly lines" />
                        
                        <StackPanel
                            Grid.Row="2" Grid.Column="0"
                            Orientation="Horizontal" HorizontalAlignment="Left">
                            <Label Margin="0,5,0,0">Count</Label>
                            <TextBox
                                Margin="10,0,0,0"
                                IsEnabled="{Binding Shared.IsStartSimulationButtonEnabled}"
                                Text="{Binding Shared.CountOfAssemblyLines}"
                                Height="20"
                                Width="40" />
                        </StackPanel>
                        
                        <Label
                            Grid.Row="3" Grid.Column="0"
                            Margin="0,10,10,5"
                            FontSize="16"
                            FontWeight="Bold"
                            FontFamily="Source Code Pro"
                            Content="Workers count" />
                        
                        <StackPanel
                            Grid.Row="4" Grid.Column="0"
                            Orientation="Horizontal" HorizontalAlignment="Left">
                            <Label Margin="0,5,0,0">A</Label>
                            <TextBox
                                Margin="10,0,0,0"
                                IsEnabled="{Binding Shared.IsStartSimulationButtonEnabled}"
                                Text="{Binding Shared.CountOfWorkersGroupA}"
                                Height="20"
                                Width="40" />
                        
                            <Label Margin="10,5,0,0">B</Label>
                            <TextBox
                                Margin="10,0,0,0"
                                IsEnabled="{Binding Shared.IsStartSimulationButtonEnabled}"
                                Text="{Binding Shared.CountOfWorkersGroupB}"
                                Height="20"
                                Width="40" />
                        
                            <Label Margin="10,5,0,0">C</Label>
                            <TextBox
                                Margin="10,0,0,0"
                                IsEnabled="{Binding Shared.IsStartSimulationButtonEnabled}"
                                Text="{Binding Shared.CountOfWorkersGroupC}"
                                Height="20"
                                Width="40" />
                        </StackPanel>
                        
                        <CheckBox
                            Grid.Row="5" Grid.Column="0"
                            IsChecked="{Binding Shared.EnableWorkerLocationPreference}"
                            IsEnabled="{Binding Shared.IsStartSimulationButtonEnabled}"
                            Margin="0,10,20,0">Worker location preference</CheckBox>
                    </Grid>
                </TabItem>
            </TabControl>

        <scottPlot:AvaPlot
            IsVisible="{Binding Shared.IsMultipleReplications}"
            Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="9"
            Margin="15,5,0,15"
            Name="ProcessingOrderTimePlot" />
    </Grid>
    
    
</UserControl>
