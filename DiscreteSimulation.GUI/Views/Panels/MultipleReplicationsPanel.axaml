                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationPendingItemsForVarnishing}" />
                
                <SelectableTextBlock
                    Grid.Row="6" Grid.Column="4"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationPendingItemsForVarnishingCI}" />
                
                <SelectableTextBlock
                    Grid.Row="6" Grid.Column="6"
                    Margin="0,0,10,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="items" />
                
                <SelectableTextBlock
                    Grid.Row="7" Grid.Column="0"
                    Margin="5,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="Pending items for folding" />
        
                <SelectableTextBlock
                    Grid.Row="7" Grid.Column="2"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationPendingItemsForFolding}" />
                
                <SelectableTextBlock
                    Grid.Row="7" Grid.Column="4"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationPendingItemsForFoldingCI}" />
                
                <SelectableTextBlock
                    Grid.Row="7" Grid.Column="6"
                    Margin="0,0,10,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="items" />
                
                <SelectableTextBlock
                    Grid.Row="8" Grid.Column="0"
                    Margin="5,0,0,10"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="Pending closets for fittings" />
        
                <SelectableTextBlock
                    Grid.Row="8" Grid.Column="2"
                    Margin="0,0,0,10"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationPendingItemsForFittings}" />
                
                <SelectableTextBlock
                    Grid.Row="8" Grid.Column="4"
                    Margin="0,0,0,10"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationPendingItemsForFittingsCI}" />
                
                <SelectableTextBlock
                    Grid.Row="8" Grid.Column="6"
                    Margin="0,0,10,10"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="items" />

                <SelectableTextBlock
                    Grid.Row="9" Grid.Column="0"
                    Margin="5,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="Waiting time pending orders" />
        
                <SelectableTextBlock
                    Grid.Row="9" Grid.Column="2"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationPendingOrdersWaitingTime}" />
                
                <SelectableTextBlock
                    Grid.Row="9" Grid.Column="4"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationPendingOrdersWaitingTimeCI}" />
                
                <SelectableTextBlock
                    Grid.Row="9" Grid.Column="6"
                    Margin="0,0,10,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="{Binding Shared.SelectedTimeUnits}" >
                    
                    <SelectableTextBlock.ContextMenu>
                        <ContextMenu >
                            <MenuItem Header="seconds" Click="MenuItem_OnClick" />
                            <MenuItem Header="minutes" Click="MenuItem_OnClick" />
                            <MenuItem Header="hours" Click="MenuItem_OnClick" />
                        </ContextMenu>
                    </SelectableTextBlock.ContextMenu>
                    
                </SelectableTextBlock>
                
                <SelectableTextBlock
                    Grid.Row="10" Grid.Column="0"
                    Margin="5,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="Waiting time pending items" />
        
                <SelectableTextBlock
                    Grid.Row="10" Grid.Column="2"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationPendingFurnitureItemsWaitingTime}" />
                
                <SelectableTextBlock
                    Grid.Row="10" Grid.Column="4"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationPendingFurnitureItemsWaitingTimeCI}" />
                
                <SelectableTextBlock
                    Grid.Row="10" Grid.Column="6"
                    Margin="0,0,10,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="{Binding Shared.SelectedTimeUnits}" >
                    
                    <SelectableTextBlock.ContextMenu>
                        <ContextMenu >
                            <MenuItem Header="seconds" Click="MenuItem_OnClick" />
                            <MenuItem Header="minutes" Click="MenuItem_OnClick" />
                            <MenuItem Header="hours" Click="MenuItem_OnClick" />
                        </ContextMenu>
                    </SelectableTextBlock.ContextMenu>
                    
                </SelectableTextBlock>

                <SelectableTextBlock
                    Grid.Row="11" Grid.Column="0"
                    Margin="5,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="Waiting time items for staining" />
        
                <SelectableTextBlock
                    Grid.Row="11" Grid.Column="2"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationItemsForStainingWaitingTime}" />
                
                <SelectableTextBlock
                    Grid.Row="11" Grid.Column="4"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationItemsForStainingWaitingTimeCI}" />
                
                <SelectableTextBlock
                    Grid.Row="11" Grid.Column="6"
                    Margin="0,0,10,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="{Binding Shared.SelectedTimeUnits}" >
                    
                    <SelectableTextBlock.ContextMenu>
                        <ContextMenu >
                            <MenuItem Header="seconds" Click="MenuItem_OnClick" />
                            <MenuItem Header="minutes" Click="MenuItem_OnClick" />
                            <MenuItem Header="hours" Click="MenuItem_OnClick" />
                        </ContextMenu>
                    </SelectableTextBlock.ContextMenu>
                    
                </SelectableTextBlock>
                
                <SelectableTextBlock
                    Grid.Row="12" Grid.Column="0"
                    Margin="5,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="Waiting time items for varnishing" />
        
                <SelectableTextBlock
                    Grid.Row="12" Grid.Column="2"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationItemsForVarnishingWaitingTime}" />
                
                <SelectableTextBlock
                    Grid.Row="12" Grid.Column="4"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationItemsForVarnishingWaitingTimeCI}" />
                
                <SelectableTextBlock
                    Grid.Row="12" Grid.Column="6"
                    Margin="0,0,10,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="{Binding Shared.SelectedTimeUnits}" >
                    
                    <SelectableTextBlock.ContextMenu>
                        <ContextMenu >
                            <MenuItem Header="seconds" Click="MenuItem_OnClick" />
                            <MenuItem Header="minutes" Click="MenuItem_OnClick" />
                            <MenuItem Header="hours" Click="MenuItem_OnClick" />
                        </ContextMenu>
                    </SelectableTextBlock.ContextMenu>
                    
                </SelectableTextBlock>
                
                <SelectableTextBlock
                    Grid.Row="13" Grid.Column="0"
                    Margin="5,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="Waiting time items for folding" />
        
                <SelectableTextBlock
                    Grid.Row="13" Grid.Column="2"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationItemsForFoldingWaitingTime}" />
                
                <SelectableTextBlock
                    Grid.Row="13" Grid.Column="4"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationItemsForFoldingWaitingTimeCI}" />
                
                <SelectableTextBlock
                    Grid.Row="13" Grid.Column="6"
                    Margin="0,0,10,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="{Binding Shared.SelectedTimeUnits}" >
                    
                    <SelectableTextBlock.ContextMenu>
                        <ContextMenu >
                            <MenuItem Header="seconds" Click="MenuItem_OnClick" />
                            <MenuItem Header="minutes" Click="MenuItem_OnClick" />
                            <MenuItem Header="hours" Click="MenuItem_OnClick" />
                        </ContextMenu>
                    </SelectableTextBlock.ContextMenu>
                    
                </SelectableTextBlock>
                
                <SelectableTextBlock
                    Grid.Row="14" Grid.Column="0"
                    Margin="5,0,0,10"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="Waiting time closets for fittings" />
        
                <SelectableTextBlock
                    Grid.Row="14" Grid.Column="2"
                    Margin="0,0,0,10"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationItemsForFittingsWaitingTime}" />
                
                <SelectableTextBlock
                    Grid.Row="14" Grid.Column="4"
                    Margin="0,0,0,10"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationItemsForFittingsWaitingTimeCI}" />
                
                <SelectableTextBlock
                    Grid.Row="14" Grid.Column="6"
                    Margin="0,0,10,10"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="{Binding Shared.SelectedTimeUnits}" >
                    
                    <SelectableTextBlock.ContextMenu>
                        <ContextMenu >
                            <MenuItem Header="seconds" Click="MenuItem_OnClick" />
                            <MenuItem Header="minutes" Click="MenuItem_OnClick" />
                            <MenuItem Header="hours" Click="MenuItem_OnClick" />
                        </ContextMenu>
                    </SelectableTextBlock.ContextMenu>
                    
                </SelectableTextBlock>
                
                <SelectableTextBlock
                    Grid.Row="15" Grid.Column="0"
                    Margin="5,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="Workers A utilization" />
                
                <SelectableTextBlock
                    Grid.Row="15" Grid.Column="2"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationWorkersAUtilization}" />
                
                <SelectableTextBlock
                    Grid.Row="15" Grid.Column="4"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationWorkersAUtilizationCI}" />
                
                <SelectableTextBlock
                    Grid.Row="15" Grid.Column="6"
                    Margin="0,0,10,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="%" />
                
                <SelectableTextBlock
                    Grid.Row="16" Grid.Column="0"
                    Margin="5,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="Workers B utilization" />
                
                <SelectableTextBlock
                    Grid.Row="16" Grid.Column="2"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationWorkersBUtilization}" />
                
                <SelectableTextBlock
                    Grid.Row="16" Grid.Column="4"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationWorkersBUtilizationCI}" />
                
                <SelectableTextBlock
                    Grid.Row="16" Grid.Column="6"
                    Margin="0,0,10,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="%" />
                
                <SelectableTextBlock
                    Grid.Row="17" Grid.Column="0"
                    Margin="5,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="Workers C utilization" />
                
                <SelectableTextBlock
                    Grid.Row="17" Grid.Column="2"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationWorkersCUtilization}" />
                
                <SelectableTextBlock
                    Grid.Row="17" Grid.Column="4"
                    Margin="0,0,0,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    HorizontalAlignment="Center"
                    Text="{Binding SimulationWorkersCUtilizationCI}" />
                
                <SelectableTextBlock
                    Grid.Row="17" Grid.Column="6"
                    Margin="0,0,10,3"
                    FontSize="14"
                    FontFamily="Source Code Pro"
                    Text="%" />
                
                <Label
                    Grid.Row="18" Grid.Column="0" Grid.ColumnSpan="3"
                    Margin="5,10,0,5"
                    FontSize="16"
                    FontFamily="Source Code Pro"
                    Content="Individual workers utilization" />
                
                <DataGrid Grid.Row="19" Grid.Column="0" Grid.ColumnSpan="7"
                          Margin="5,0,10,66" 
                          ItemsSource="{Binding SimulationAllWorkersUtilization}"
                          SelectionMode="Single"
                          CanUserSortColumns="False"
                          IsReadOnly="True"
                          GridLinesVisibility="All"
                          RowHeight="24"
                          BorderThickness="1" BorderBrush="Gray">
            
                    <DataGrid.Styles>
                        <Style Selector="DataGridColumnHeader">
                            <Setter Property="FontWeight" Value="Bold"/>
                        </Style>
                        <Style Selector="DataGridCell:current /template/ Grid#FocusVisual"> 
                            <Setter Property="IsVisible" Value="false" /> 
                        </Style> 
                    </DataGrid.Styles>
            
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="ID" Width="Auto" Binding="{Binding Id}" />
                        <DataGridTextColumn Header="Group" Width="Auto" Binding="{Binding State}" />
                        <DataGridTextColumn Header="Utilization" Width="Auto" Binding="{Binding Utilization}" />
                        <DataGridTextColumn Header="Utilization 95% CI" Width="*" Binding="{Binding Place}" />
                    </DataGrid.Columns>
                </DataGrid>
        
            </Grid>
        
        <scottPlot:AvaPlot
            IsVisible="{Binding Shared.IsMultipleReplications}"
            Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="9"
            Margin="15,10,0,15"
            Name="ProcessingOrderTimePlot" />
    </Grid>
    
    
</UserControl>
